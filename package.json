{
  "name": "shoptet-css-proxy",
  "version": "1.0.0",
  "description": "A development proxy server that intercepts CSS requests and serves local CSS files while proxying other requests to a production server",
  "main": "index.js",
  "type": "module",
  "bin": {
    "shoptet-css-proxy": "./bin/cli.js"
  },
  "files": [
    "index.js",
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "node proxy-server.js",
    "start": "node bin/cli.js"
  },
  "keywords": [
    "proxy",
    "css",
    "development",
    "shoptet",
    "hot-reload",
    "local-development"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "chokidar": "^3.6.0",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "http-proxy-middleware": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "javascript-obfuscator": "^4.1.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.8.0",
    "path": "^0.12.7",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "webpack-obfuscator": "^3.5.1",
    "webpack-remove-empty-scripts": "^1.0.4"
  }
}
